<Page x:Class="Operation_Transform.ListPages.CProgramsList"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:local="using:Operation_Transform.ListPages"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d"
      Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid x:Name="LayoutRoot"
          Background="{StaticResource ApplicationBackgroundColor}">

        <Grid.ChildrenTransitions>
            <TransitionCollection>
                <EntranceThemeTransition />
            </TransitionCollection>
        </Grid.ChildrenTransitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <!-- Title Panel -->
        <StackPanel Grid.Row="0"
                    Margin="19,0,0,0">
            <TextBlock Text="{StaticResource C}"
                       Style="{StaticResource ProgramListMenuHeader}"
                       CharacterSpacing="{ThemeResource PivotHeaderItemCharacterSpacing}" />
        </StackPanel>

        <!--TODO: Content should be placed within the following grid-->
        <ScrollViewer Style="{StaticResource ProgramListScrollViewer}">
            <StackPanel>
                <Button x:Name="HelloWorld"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Hello World"
                        Tag="HelloWorld"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="Comments"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Comments"
                        Tag="Comments"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="SwapUsingTemporaryVariable"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Swap (Using Temporary Variable)"
                        Tag="SwapUsingTemporaryVariable"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="isEven"
                        Style="{StaticResource ProgramListButtons}"
                        Content="isEven?"
                        Tag="isEven"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="isOdd"
                        Style="{StaticResource ProgramListButtons}"
                        Content="isOdd?"
                        Tag="isOdd"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="LeapYear"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Leap Year"
                        Tag="LeapYear"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="SimpleInterestAndAmount"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Simple Interest &amp; Amount"
                        Tag="SimpleInterestAndAmount"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="ArithmeticProgression"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Arithmetic Progression"
                        Tag="ArithmeticProgression"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="GeometricProgression"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Geometric Progression"
                        Tag="GeometricProgression"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="isPrime"
                        Style="{StaticResource ProgramListButtons}"
                        Content="isPrime?"
                        Tag="isPrime"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="NumberReversal"
                        Style="{StaticResource ProgramListButtons}"
                        Content="NumberReversal"
                        Tag="NumberReversal"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="RootsOfQuadraticEquation"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Quadratic Equation (Roots)"
                        Tag="RootsOfQuadraticEquation"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="PascalTriangle"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Pascal's Triangle"
                        Tag="PascalTriangle"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="IterativeFactorial"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Factorial (Iterative)"
                        Tag="IterativeFactorial"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="FibonacciSeries"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Fibonacci Series"
                        Tag="FibonacciSeries"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="recursiveFactorial"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Factorial (Recursion)"
                        Tag="recursiveFactorial"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="recursiveBinarySearch"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Binary Search (Recursion)"
                        Tag="recursiveBinarySearch"

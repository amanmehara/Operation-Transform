                        Tag="FirstComeFirstServeScheduling"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="ShortestJobFirstScheduling"
                        Style="{StaticResource ProgramListButtons}"
                        Content="SJF (Scheduling)"
                        Tag="ShortestJobFirstScheduling"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="PriorityScheduling"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Priority (Scheduling)"
                        Tag="PriorityScheduling"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="RoundRobinScheduling"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Round Robin (Scheduling)"
                        Tag="RoundRobinScheduling"
                        Click="CCodeNavigation_Click" />
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Page>

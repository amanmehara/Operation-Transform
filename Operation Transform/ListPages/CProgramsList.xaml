                        Tag="QueueUsing2Stacks"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="SortingArrayUsing2Stacks"
                        Style="{StaticResource ProgramListButtons}"
                        Content="Sorting Array (Using 2 Stacks)"
                        Tag="SortingArrayUsing2Stacks"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="forkProcessSystemCall"
                        Style="{StaticResource ProgramListButtons}"
                        Content="fork (Process System Call)"
                        Tag="forkProcessSystemCall"
                        Click="CCodeNavigation_Click" />
                <Button x:Name="waitProcessSystemCall"
                        Style="{StaticResource ProgramListButtons}"
